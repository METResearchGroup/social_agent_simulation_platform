      "RunResponseStatus": {
        "description": "Status of a simulation run as returned by the API.",
        "enum": [
          "completed",
          "failed"
        ],
        "title": "RunResponseStatus",
        "type": "string"
      },
      "RunStatus": {
        "description": "Enum representing the state of a simulation run.\n\nState transitions:\n  - RUNNING: The run is actively in progress. All runs start in this state.\n  - COMPLETED: The run has finished successfully. Set when the simulation\n    completes all turns and agents have completed their actions.\n  - FAILED: The run has terminated abnormally due to an error or interruption.\n    No further progress will be made.\n\nValid transitions:\n  - RUNNING -> COMPLETED: Normal successful completion.\n  - RUNNING -> FAILED: Error or failure during simulation.",
        "enum": [
          "running",
          "completed",
          "failed"
        ],
        "title": "RunStatus",
        "type": "string"
      },
