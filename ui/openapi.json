    "/v1/simulations/agents": {
      "get": {
        "description": "Return simulation agent profiles from DB for View agents and Create form.",
        "operationId": "get_simulation_agents_v1_simulations_agents_get",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/AgentSchema"
                  },
                  "title": "Response Get Simulation Agents V1 Simulations Agents Get",
                  "type": "array"
                }
              }
            },
            "description": "Successful Response"
          }
        },
        "security": [
          {
            "HTTPBearer": []
          }
        ],
        "summary": "List simulation agents",
        "tags": [
          "simulation"
        ]
      },
      "post": {
        "description": "Create a user-generated agent.",
        "operationId": "post_simulation_agents_v1_simulations_agents_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateAgentRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AgentSchema"
                }
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "security": [
          {
            "HTTPBearer": []
          }
        ],
        "summary": "Create simulation agent",
        "tags": [
          "simulation"
        ]
      }
    },
